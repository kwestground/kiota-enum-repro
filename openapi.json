{
  "openapi": "3.0.3",
  "info": {
    "title": "Kiota Enum Import Repro",
    "version": "1.0.0"
  },
  "paths": {
    "/items": {
      "get": {
        "operationId": "getItems",
        "parameters": [
          {
            "name": "status",
            "in": "query",
            "schema": {
              "$ref": "#/components/schemas/ItemStatus"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "List of items",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Item"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ItemStatus": {
        "type": "string",
        "enum": ["Draft", "Active", "Archived"]
      },
      "Item": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "name": {
            "type": "string"
          },
          "status": {
            "$ref": "#/components/schemas/ItemStatus"
          }
        }
      }
    }
  }
}
